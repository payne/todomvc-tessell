<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:gwt="urn:import:com.google.gwt.user.client.ui">
  <ui:with field="bs" type="org.tessell.todomvc.client.resources.BaseStyle" />
  <gwt:HTMLPanel>
    <gwt:SimpleCheckBox ui:field="toggleAll" styleName="{bs.toggleAll} "/>
    <label class="{bs.toggleAll}">Mark all as complete</label><!-- Not currently hooked up to the checkbox... -->
    <gwt:FlowPanel ui:field="ul" tag="ul" styleName="{bs.todoList}" />
  </gwt:HTMLPanel>
</ui:UiBinder>